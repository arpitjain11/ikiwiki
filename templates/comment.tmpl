<TMPL_VAR NAME=COMMENTAUTHOR>
</TMPL_IF>
</span>
</TMPL_IF>
<TMPL_ELSE><!-- !COMMENTUSER -->
<TMPL_IF NAME=COMMENTIP>
<span class="author" title="Unauthenticated, from <TMPL_VAR NAME=COMMENTIP>">
